{
  "name": "survivorsunited.org",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "watch": "docusaurus start",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:headless": "cypress run --headless",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:edge": "cypress run --browser edge",
    "test:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "test:tablet": "cypress run --config viewportWidth=768,viewportHeight=1024",
    "test:desktop": "cypress run --config viewportWidth=1280,viewportHeight=720",
    "test:navigation": "cypress run --spec 'cypress/e2e/navigation.cy.js'",
    "test:pages": "cypress run --spec 'cypress/e2e/pages.cy.js'",
    "test:search": "cypress run --spec 'cypress/e2e/search.cy.js'",
    "test:links": "cypress run --spec 'cypress/e2e/links.cy.js'",
    "test:responsive": "cypress run --spec 'cypress/e2e/responsive.cy.js'",
    "test:all": "cypress run --spec 'cypress/e2e/*.cy.js'",
    "test:record": "cypress run --record",
    "test:parallel": "cypress run --parallel",
    "test:ci": "start-server-and-test start http://localhost:3000 test:headless",
    "modpack:sync": "tsx scripts/modpack-sync.ts"
  },
  "dependencies": {
    "@docusaurus/core": "3.1.1",
    "@docusaurus/preset-classic": "3.1.1",
    "@easyops-cn/docusaurus-search-local": "^0.51.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.5.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.1.1",
    "@docusaurus/types": "3.1.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "adm-zip": "^0.5.16",
    "cypress": "^14.5.0",
    "nodemon": "^3.0.3",
    "sharp": "^0.34.2",
    "start-server-and-test": "^2.0.12",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "wait-on": "^8.0.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
